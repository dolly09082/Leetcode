class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        int s = 0;
        int e = matrix[0].length-1;

        while(s <= matrix.length-1 && e >= 0){
            if(matrix[s][e] == target) return true;
            else if(matrix[s][e] < target) s++;
            else e--;
        }

        return false;
    }
}
