class Solution {
    public List<Integer> findDuplicates(int[] arr) {
        int i=0;
        while(i<arr.length){
            int correct = arr[i]-1;
            if(arr[i] != arr[correct]) {
                swap(arr, i, correct);
            }
            else{
                i++;
            }
        }

        List<Integer> ans = new ArrayList<>();
        for(i=0;i<arr.length; i++){
            if(i != arr[i]-1) 
                ans.add(arr[i]);
        }
        return ans;
    }

    public void swap(int[] arr, int first, int second){
        int temp = arr[first];
        arr[first] = arr[second];
        arr[second] = temp;
    }
}
